python3 experiments/torchtext_runner.py --project vocab_redux_onehot --tokenizer ekphrasis --experiment liwc --encoding onehot --model mlp --main wulczyn --aux davidson garcia waseem waseem_hovy --gpu 0 --patience 50 --layers 1 --datadir data/json/ --results results/ --save_model results/models/ --shuffle True --cleaners username hashtag url lower --metrics f1_score accuracy precision recall --optimizer adam --loss nlll --seed 42 --batch_size 64 --dropout 0.1 --embedding 200 --epochs 50 --hidden 200 --learning_rate 0.2 --nonlinearity relu

